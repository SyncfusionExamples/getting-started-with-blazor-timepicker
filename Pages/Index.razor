@page "/"

<div id="wrapper">
    <SfTimePicker TValue="DateTime?" Value="@TimeValue"
                  Min="@MinTime"
                  Max="@MaxTime"
                  Format="HH:mm"
                  Step="60"></SfTimePicker>
</div>

@code {
    public DateTime TimeValue { get; set; } = new DateTime(2020, 03, 25, 05, 00, 00);

    public DateTime MinTime { get; set; } = new DateTime(2020, 03, 25, 01, 00, 00);

    public DateTime MaxTime { get; set; } = new DateTime(2020, 03, 25, 05, 00, 00);
}

<style>
    #wrapper {
        max-width: 300px;
        margin: 0px auto;
        padding-top: 20px;
    }
</style>